  object QBrowse: TOracleDataSet
    SQL.Strings = (
      'select * from '
      '('
      'select a.no_alat, '
      'a.jenis, '
      'a.nama_alat, '
      'a.tgl_perolehan, 1 as vcount,'
      'a.isaktif, b.awal, c.debet, c.kredit from alat a '
      
        'left outer join (select p.no_alat, sum(p.debet-p.kredit) as awal' +
        ' '
      
        '  from jurnal p, kas q where p.no_kas=q.no_kas and q.tanggal>=tr' +
        'unc(:pawal,'#39'YEAR'#39') and q.tanggal<trunc(:pawal)'
      '  group by p.no_alat) b on (a.no_alat=b.no_alat)'
      
        'left outer join (select p.no_alat, sum(p.debet) as debet, sum(kr' +
        'edit) as kredit'
      
        '  from jurnal p, kas q where p.no_kas=q.no_kas and q.tanggal>=tr' +
        'unc(:pawal) and q.tanggal<trunc(:pakhir)+1-1/86400'
      '  group by p.no_alat) c on (a.no_alat=c.no_alat)'
      ')'
      ':myparam')
    Variables.Data = {
